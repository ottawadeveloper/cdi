<?php

class CDIGuidLoadCommand extends CDIAbstractDataTypeCommand {
  
  private $do = NULL;
  private $guid = NULL;
  
  public function __construct($guid) {
    parent::__construct(array('CIDGuidLoadInterface'));
    $this->guid = $guid;
  }
  
  public function toString() {
    return 'Load by GUID';
  }
  
  public function executeAction(CDIReceiverInterface $receiver) {
    $this->do = $receiver->loadByGUID($this->guid);
  }
  
  public function getDataObject() {
    return $this->do;
  }
  
}