<?php

class CDIGuidDeleteCommand extends CDIAbstractDataObjectCommand {
  
  private $result;
  
  public function __construct() {
    parent::__construct(array('CDIGuidDeleteInterface'));
  }
  
  public function toString() {
    return 'GUID Delete';
  }
  
  public function executeAction(CDIReceiverInterface $receiver) {
    $this->result = $receiver->deleteByGUID($this->getDataObject());
  }
  
  public function getResult() {
    return $this->result;
  }
  
}