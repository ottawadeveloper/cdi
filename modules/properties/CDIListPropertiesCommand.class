<?php

class CDIListPropertiesCommand extends CDIAbstractDataTypeCommand {
  
  private $properties = array();
  
  public function __construct() {
    parent::_construct(array('CDIListPropertiesInterface'));
  }
  
  public function toString() {
    return 'List Properties';
  }
  
  public function executeAction(CDIReceiverInterface $receiver) {
    $this->properties = $receiver->listProperties();
  }
  
  public function getProperties() {
    return $this->properties;
  }
  
}