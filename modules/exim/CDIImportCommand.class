<?php

class CDIImportCommand extends CDIAbstractDataTypeCommand {
  
  private $chunk = NULL;
  private $do = NULL;
  
  public function __construct(CDIExportChunk $chunk) {
    parent::__construct('CDIImportInterface');
    $this->chunk = $chunk;
  }
  
  public function toString() {
    return 'Import';
  }
  
  public function executeAction(CDIReceiverInterface $receiver) {
    $this->do = $receiver->import($this->chunk->getExportData());
  }
  
  public function getDataObject() {
    return $this->do;
  }
  
}