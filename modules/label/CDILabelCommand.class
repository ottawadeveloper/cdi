<?php

class CDILabelCommand extends CDIAbstractDataObjectCommand {
  
  private $label = '';
  
  public function __construct() {
    parent::__construct(array('CDILabelInterface'));
  }
  
  public function toString() {
    return 'Label';
  }
  
  public function executeAction(CDIReceiverInterface $receiver) {
    $this->label = $receiver->label($this->getDataObject());
  }
  
  public function getLabel() {
    return $this->label;
  }
  
}
