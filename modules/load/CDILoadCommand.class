<?php

class CDILoadCommand extends CDIAbstractDataTypeCommand {
  
  private $id;
  private $subtype;
  private $object;
  
  public function __construct($id, $subtype) {
    parent::__construct(array('CDILoadInterface'));
    $this->id = $id;
    $this->subtype = $subtype;
  }
  
  public function executeAction(CDIReceiverInterface $receiver) {
    $this->object = $receiver->loadObject($this->id, $this->subtype);
  }
  
  public function getObject() {
    return $this->object;
  }
  
  public function toString() {
    return 'Load command';
  }
  
}