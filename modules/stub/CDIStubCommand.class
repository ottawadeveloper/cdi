<?php

class CDIStubCommand extends CDIAbstractDataTypeCommand {
  
  private $object;
  
  private $initialValues = array();
  
  public function __construct($defaults = array()) {
    parent::__construct(array('CDIStubInterface'));
    $this->initialValues = $defaults;
  }
  
  public function toString() {
    return 'Stub';
  }
  
  public function executeAction(CDIReceiverInterface $receiver) {
    $this->object = $receiver->stub($this->initialValues);
  }
  
  public function getDataObject() {
    return $this->object;
  }
  
}
