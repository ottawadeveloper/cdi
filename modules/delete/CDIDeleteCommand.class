<?php

class CDIDeleteCommand extends CDIAbstractDataObjectCommand {
  
  private $results = NULL;
  
  public function __construct() {
    parent::__construct(array('CDIDeleteInterface'));
  }
  
  public function toString() {
    return 'Delete Command';
  }
  
  public function executeAction(CDIReceiverInterface $receiver) {
    $this->results = $receiver->delete($this->getDataObject());
  }
  
  public function getResult() {
    return $this->results;
  }
  
}